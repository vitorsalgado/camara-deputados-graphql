{
  "name": "camara-federal-graphql",
  "version": "1.0.0",
  "description": "GraphQL for the Brazilian Chamber of Deputies REST API",
  "private": true,
  "main": "./dist/cmd/srv/index.js",
  "types": "./dist/cmd/srv/index.d.ts",
  "scripts": {
    "start": "DIST_MODE=1 node dist/cmd/srv/index.js",
    "start:dev": "nodemon --ext js,ts,json,graphql,env src/cmd/srv/index.ts",
    "test": "jest --detectOpenHandles --logHeapUsage",
    "test:ci": "yarn test --colors --coverage --ci",
    "test:coverage:upload": "codecov -f coverage/*.json",
    "build": "tsc --project tsconfig.build.json && node scripts/copy-schemas",
    "postinstall": "husky install",
    "lint": "eslint --ext .ts .",
    "prettier": "prettier 'src/**' --write --ignore-unknown",
    "prettier:ci": "prettier --check 'src/**' --ignore-unknown"
  },
  "engines": {
    "node": ">=14.17",
    "yarn": ">=2.4.2"
  },
  "author": {
    "name": "Vitor Hugo Salgado",
    "email": "vsalgadopb@gmail.com",
    "url": "https://github.com/vitorsalgado"
  },
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vitorsalgado/camara-deputados-graphql.git"
  },
  "bugs": {
    "url": "https://github.com/vitorsalgado/camara-deputados-graphql/issues"
  },
  "homepage": "https://github.com/vitorsalgado/camara-deputados-graphql#readme",
  "dependencies": {
    "dotenv": "^10.0.0",
    "drizzle-http": "^2.2.0",
    "fastify": "^3.18.0",
    "ioredis": "^4.27.6",
    "joi": "^17.4.0",
    "mercurius": "^7.9.1",
    "pino": "^6.11.3",
    "pino-pretty": "^5.0.2",
    "tsconfig-paths": "^3.9.0",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@types/ioredis": "^4.26.4",
    "@types/jest": "^26.0.23",
    "@types/node": "^15.12.4",
    "@types/pino": "^6.3.8",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^4.28.1",
    "@typescript-eslint/parser": "^4.27.0",
    "eslint": "^7.29.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-tsdoc": "^0.2.14",
    "fs-extra": "^10.0.0",
    "globby": "^11.0.4",
    "husky": "^6.0.0",
    "jest": "^27.0.4",
    "lint-staged": "^11.0.0",
    "nock": "^13.1.0",
    "nodemon": "^2.0.7",
    "prettier": "2.3.1",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.3",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.4"
  }
}
